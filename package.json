{
  "name": "dont-starve-planner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs && touch docs/.nojekyll",
    "pub": "npm run build && git add . && git commit -m \"Publish\"",
